/* pages/help/help.wxss */

.danzhu-wechat {
    font-family: danzhu !important;
    font-size: 40rpx !important;
    font-style: normal;
    color: #02c161 !important;
}

.main-container {
    margin-top: 30rpx;
    margin-left: 40rpx;
    box-sizing: border-box;
    width: 680rpx;
}

.l-list {
    border-bottom: 0 !important;
    margin-top: 30rpx !important;
    color: #000000 !important;
}

.bottom-container {
    width: 100%;
    height: 108rpx;
    position: fixed;
    bottom: 0;
    display: flex;
    align-items: center;
    justify-content: center;
    flex-direction: row;
    background-color: #e1f3ff;
}

.wechat-text {
    color: #212121;
    font-weight: 900;
    font-size: 30rpx;
    margin-left: 10rpx;
}