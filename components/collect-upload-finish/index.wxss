
@import "../../wxss/danzhu.wxss";

.container {
    display: flex;
    flex-direction: column;
    align-items: center;
}

.image {
    margin-top: 160rpx;
    width: 150rpx;
    height: 150rpx;
}

.tip-title {
    margin-top: 40rpx;
    color: #212121;
    font-size: 28rpx;
}

.tip {
    margin-top: 20rpx;
    color: #9e9e9e;
    font-size: 24rpx;
}

.tip1 {
    margin-right: 20rpx;
}

.btn-container {
    margin-top: 180rpx;
    display: flex;
    flex-direction: column;
    align-items: center;
    font-size: 26rpx;
}

.again-select {
    display: flex;
    flex-direction: row;
    align-items: center;
    justify-content: center;
    background-color: #09AAFF;
    color: #ffffff;
    height: 56rpx;
    width: 250rpx;
    padding: 10rpx 10rpx;
    border-radius: 100rpx;
}

.sent-file {
    margin-top: 40rpx;
    color: #09AAFF;
}