
.container {
    font-size: 26rpx;
    color: #212121;
    display: flex;
    flex-direction: column;
    max-height: 500rpx;
    min-height: 300rpx;
    background-color: #ffffff;
}

.main-conatiner {
    display: flex;
    flex-direction: row;
    align-items: center;
    justify-content: space-evenly;
    padding: 50rpx;
}

.content {
    display: flex;
    flex-direction: column;
    align-items: center;
}

.content image {
    width: 64rpx;
    height: 64rpx;
    padding: 10rpx;
    background-color: #f5f5f5;
    border-radius: 50%;
    margin-bottom: 10rpx;
}

.cancel {
    position: fixed;
    bottom: 0;
    display: flex;
    align-items: center;
    flex-direction: column;
    justify-content: center;
    width: 92%;
    height: 100rpx;
    background-color: #f9f9f9;
    color: #212121;
    font-size: 30rpx;
    font-weight: bold;
}